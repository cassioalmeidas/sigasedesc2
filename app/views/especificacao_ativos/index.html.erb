<p id="notice"><%= notice %></p>

<h1>Especificacao Ativos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @especificacao_ativos.each do |especificacao_ativo| %>
      <tr>
        <td><%= especificacao_ativo.nome %></td>
        <td><%= link_to 'Show', especificacao_ativo %></td>
        <td><%= link_to 'Edit', edit_especificacao_ativo_path(especificacao_ativo) %></td>
        <td><%= link_to 'Destroy', especificacao_ativo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Especificacao Ativo', new_especificacao_ativo_path %>
