<p id="notice"><%= notice %></p>

<h1>Ti Tecnicos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ti_tecnicos.each do |ti_tecnico| %>
      <tr>
        <td><%= ti_tecnico.nome %></td>
        <td><%= link_to 'Show', ti_tecnico %></td>
        <td><%= link_to 'Edit', edit_ti_tecnico_path(ti_tecnico) %></td>
        <td><%= link_to 'Destroy', ti_tecnico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ti Tecnico', new_ti_tecnico_path %>
